{
	"compilerOptions": {
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"lib": ["ESNext"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "react"
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "build"]
}
