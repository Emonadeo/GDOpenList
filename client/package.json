		"fuzzysort": "^2.0.1",
		"modern-css-reset": "^1.4.0",
		"postcss": "^8.4.14",
		"sass": "^1.54.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tinro": "^0.6.12",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.4"
	}
}
